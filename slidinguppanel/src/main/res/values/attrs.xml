<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlidingUpPanelLayout">
        <attr name="slidingUpPanelHeight" format="dimension" />
        <attr name="slidingUpShadowHeight" format="dimension" />
        <attr name="slidingUpParallaxOffset" format="dimension" />
        <attr name="slidingUpFadeColor" format="color" />
        <attr name="slidingUpFlingVelocity" format="integer" />
        <attr name="slidingUpDragView" format="reference" />
        <attr name="slidingUpScrollableView" format="reference" />
        <attr name="slidingUpOverlay" format="boolean"/>
        <attr name="slidingUpClipPanel" format="boolean"/>
        <attr name="slidingUpAnchorPoint" format="float" />
        <attr name="slidingUpInitialState" format="enum">
            <enum name="expanded" value="0" />
            <enum name="collapsed" value="1" />
            <enum name="anchored" value="2" />
            <enum name="hidden" value="3" />
        </attr>
        <attr name="slidingUpScrollInterpolator" format="reference" />
    </declare-styleable>
</resources>